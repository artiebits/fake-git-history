{
  "name": "fake-git-history",
  "version": "1.1.0-beta.0",
  "description": "A command-line tool to generate your git activity",
  "bin": "src/cli.js",
  "main": "src/cli.js",
  "files": [
    "src/cli.js",
    "src/index.js"
  ],
  "repository": "git@github.com:artiebits/fake-git-history.git",
  "author": "artiebits <artur.khusaenov@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node src/cli.js",
    "lint": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*src/**/*.{js,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "chalk": "4.1.0",
    "date-fns": "1.30.1",
    "meow": "5.0.0",
    "ora": "5.1.0"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^1.19.1"
  },
  "keywords": [
    "git",
    "github",
    "commit-history",
    "git-activity",
    "contributions-calendar",
    "contribution-graph",
    "contributions"
  ]
}
